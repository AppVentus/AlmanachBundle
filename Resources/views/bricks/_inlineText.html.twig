{##
 # ALLOWED PARAMETERS
 # tags : [Marked, Deleted, Strike, Inserted, Underlined, Small, Bold, Italic]
 #}

{% set inline = ['Marked', 'Deleted', 'Strike', 'Inserted', 'Underlined', 'Small', 'Bold', 'Italic'] %}

{% for _tag in inline %}
    {% embed "AlmanachBundle:bricks:#{framework}/_inlineText.html.twig" with {framework: framework, inline: _tag} only %}
        {% block content %} {{ inline }} {% endblock %}
    {% endembed %}
    <br>
{% endfor %}