{% set almanach = attribute(almanach, framework) %}

<{{ attribute(almanach.list.order, order is defined ? order : 'default') }} class="{{ attribute(almanach.list.decoration, decoration is defined ? decoration : 'default') }}">
    {% block content %}
        {% for item in content %}
            {{ item|raw }}
        {% endfor %}
    {% endblock %}
</{{ attribute(almanach.list.order, order is defined ? order : 'default') }}>